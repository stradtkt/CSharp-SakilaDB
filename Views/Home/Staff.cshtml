@model IEnumerable<Staff>

<div class="row">
    <div class="col-md-10">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Staff ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Username</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var person in Model)
                {
                    <tr>
                        <td>@person.staff_id</td>
                        <td>@person.first_name</td>
                        <td>@person.last_name</td>
                        <td>@person.username</td>
                        <td>@person.email</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>